{
  "name": "censys.io",
  "version": "0.0.3",
  "description": "Javascript wrapper for Censys.io API",
  "homepage": "https://github.com/b4dnewz/node-censys",
  "author": {
    "name": "Filippo Conti",
    "email": "filippo@codekraft.it",
    "url": "https://b4dnewz.github.io/"
  },
  "files": ["lib", "bin", "build"],
  "main": "./build/censys.js",
  "bin": {
    "censys": "./bin/index.js"
  },
  "keywords": ["censys"],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.20.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    "lint-staged": "^7.0.0",
    "nock": "^9.2.1",
    "nsp": "^3.2.1",
    "prettier": "^1.10.2",
    "regenerator-runtime": "^0.11.1",
    "webpack-cli": "^2.0.9"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest --coverage",
    "build": "webpack"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "b4dnewz/node-censys",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "^2.14.1",
    "node-fetch": "^2.0.0"
  }
}
