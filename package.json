{
  "name": "censys.io",
  "version": "0.0.2",
  "description": "Javascript wrapper for Censys.io API",
  "homepage": "https://github.com/b4dnewz/node-censys",
  "author": {
    "name": "Filippo Conti",
    "email": "filippo@codekraft.it",
    "url": "https://b4dnewz.github.io/"
  },
  "files": [
    "lib",
    "bin",
    "build"
  ],
  "main": "./build/censys.js",
  "bin": {
    "censys": "./bin/index.js"
  },
  "keywords": [
    "censys"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^2.13.1",
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "lint-staged": "^4.2.1",
    "nock": "^9.1.6",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "regenerator-runtime": "^0.11.1",
    "webpack": "^3.10.0"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest --coverage",
    "build": "webpack"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "b4dnewz/node-censys",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "^2.13.0",
    "node-fetch": "^1.7.3"
  }
}
