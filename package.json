{
  "name": "censys.io",
  "version": "0.0.5",
  "description": "Javascript wrapper for Censys.io API",
  "homepage": "https://github.com/b4dnewz/node-censys",
  "author": {
    "name": "Filippo Conti",
    "email": "filippo@codekraft.it",
    "url": "https://b4dnewz.github.io/"
  },
  "license": "MIT",
  "files": [
    "bin",
    "build"
  ],
  "main": "./build/censys.js",
  "bin": {
    "censys": "./bin/index.js"
  },
  "keywords": [
    "censys",
    "javascript-client",
    "node-client"
  ],
  "dependencies": {
    "commander": "^2.19.0",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "babel-loader": "^8.0.5",
    "eslint": "^5.15.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.5",
    "nock": "^10.0.6",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "scripts": {
    "pretest": "npm run lint",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prebuild": "npm run test",
    "test": "jest --coverage",
    "lint": "eslint . --fix",
    "build": "webpack",
    "dev": "webpack --watch"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "jest": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    }
  },
  "repository": "b4dnewz/node-censys",
  "jest": {
    "testEnvironment": "node"
  }
}
